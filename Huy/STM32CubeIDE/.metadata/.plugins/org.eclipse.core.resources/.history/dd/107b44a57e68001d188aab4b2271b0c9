#include "global.h"

int LED_RED_init = 5, LED_YELLOW_init = 2, LED_GREEN_init = 3;
int status1 = 0, status2 = 0;
int mode = 1;

void toggleLED(){
	if(timer_flag[2] == 1){
		setTimer(1000 , 3);
		HAL_GPIO_TogglePin(TOGGLE_LED_GPIO_Port, TOGGLE_LED_Pin);
	}
}
