#ifndef INC_DISPLAY7SEG_H_
#define INC_DISPLAY7SEG_H_

#include "display7SEG1.h"
#include "display7SEG2.h"

void display7SEG(int num){
	if(num<10) {
		display7SEG1(0);
		display7SEG2(num);
	}
	else{
		display7SEG1(num - num%10);
		display7SEG2(num%10);
	}
}
#endif /* INC_DISPLAY7SEG_H_ */
