#include "fsm_automatic.h"
int SevenSEGCounter = 0;

void fsm_automatic_run(){
	setTimer1(1000);
	display7SEG(SevenSEGCounter);
	if(timer1_flag == 1){
		setTimer1(1000);
		//TODO
		if(SevenSEGCounter>0) counter--;
		display7SEG(SevenSEGCounter);
	}

}

